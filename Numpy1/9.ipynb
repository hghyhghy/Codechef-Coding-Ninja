    "import numpy as np\n",
    "\n",
    "a= np.arange(12)\n",
    "\n",
    "a=a.reshape(3,4)\n",
    "print(\"The original array is \", a)\n",
    "\n",
    "# modifying the matrix\n",
    "\n",
    "for x in np.nditer(a , op_flags=['readwrite']):\n",
    "    x[...] =5*x\n",
    "\n",
    "    print(x)\n",
    "\n",
    "for y in np.nditer(a, flags=['external_loop'], order=\"C\"):\n",
    "\n",
    "    print(y)"
   ]
  }
