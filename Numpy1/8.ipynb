    "import numpy as np\n",
    "\n",
    "# arranging in c order \n",
    "a= np.arange(10)\n",
    "\n",
    "a=a.reshape(2,5)\n",
    "\n",
    "print(\"New matrix is \", a)\n",
    "\n",
    "# iterating over array \n",
    "for x in np.nditer(a,order='C'):\n",
    "\n",
    "    print(x)\n",
    "\n",
    "# arranging through F order\n",
    "\n",
    "for y in np.nditer(a,order='F'):\n",
    "\n",
    "    print(y)"
   ]
  }
